set(TOOLBOX_VERSION @PROJECT_VERSION@)

@PACKAGE_INIT@

set_and_check(TOOLBOX_INCLUDE_DIR "@PACKAGE_INSTALL_INCLUDE_DIR@")
set_and_check(TOOLBOX_LIB_DIR "@PACKAGE_INSTALL_LIB_DIR@")

if(NOT "@target_dependencies@" STREQUAL "")
	include(CMakeFindDependencyMacro)

	foreach(dependency "@target_dependencies@")
	  find_dependency(${dependency})
	endforeach()
endif()

include("@PACKAGE_INSTALL_CMAKE_DIR@/@PROJECT_NAME@-targets.cmake")